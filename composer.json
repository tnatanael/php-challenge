{
    "name": "jobsity/slim-challenge",
    "description": "PHP Challenge - Jobsity",
    "type": "project",
    "require": {
        "php": "^7.3 || ^8.0",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "php-di/php-di": "^6.3",
        "symfony/dotenv": "^5.2",
        "symfony/mailer": "^6.0",
        "symfony/mime": "^6.0",
        "php-amqplib/php-amqplib": "^3.0",
        "ext-json": "*",
        "illuminate/database": "^12.20",
        "illuminate/events": "^12.20",
        "illuminate/container": "^12.20",
        "illuminate/support": "^12.20",
        "zircote/swagger-php": "^5.1",
        "symfony/var-dumper": "^7.3",
        "firebase/php-jwt": "^6.11"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
